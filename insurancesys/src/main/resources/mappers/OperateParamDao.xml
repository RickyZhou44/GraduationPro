<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN" "http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">
<mapper namespace="whu.iss.insurancesys.dao.HumanResourceParamDaos.OperateParamDao">

    <update id="update"
            parameterType="whu.iss.insurancesys.entity.HumanResourceParamEntities.OperateParam">
        update operate_param
        <set>
            <if test="code != null">code = #{code}</if>
            <if test="name != null">name = #{name}</if>
        </set>
        <where>
            <if test="code != null">code = #{code}</if>
        </where>
        ;
    </update>

    <select id="getAll" parameterType="whu.iss.insurancesys.entity.HumanResourceParamEntities.OperateParam">
        select * from operate_param;
    </select>

</mapper>