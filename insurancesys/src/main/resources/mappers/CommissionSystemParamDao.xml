<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN" "http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">
<mapper namespace="whu.iss.insurancesys.dao.HumanResourceParamDaos.CommissionSystemParamDao">

    <update id="update"
            parameterType="whu.iss.insurancesys.entity.HumanResourceParamEntities.CommissionSystemParam">
        update commission_system_param
        <set>
            <if test="code != null">code = #{code}</if>
            <if test="name != null">name = #{name}</if>
            <if test="create_time != null">create_time = #{createTime}</if>
        </set>
        <where>
            <if test="name != null">name = #{name}</if>
        </where>
        ;
    </update>

    <select id="getAll" parameterType="whu.iss.insurancesys.entity.HumanResourceParamEntities.CommissionSystemParam">
        select * from commission_system_param;
    </select>

</mapper>