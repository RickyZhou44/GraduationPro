<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="whu.iss.insurancesys.dao.DepartmentInfoMapper">
    <resultMap id="BaseResultMap" type="whu.iss.insurancesys.entity.DepartmentInfo">
        <result column="dept_id" jdbcType="INTEGER" property="deptId"/>
        <result column="cname" jdbcType="VARCHAR" property="cname"/>
        <result column="sname" jdbcType="VARCHAR" property="sname"/>
        <result column="branch_id" jdbcType="INTEGER" property="branchId"/>
    </resultMap>
    <insert id="insert" parameterType="whu.iss.insurancesys.entity.DepartmentInfo">
    insert into department_info (dept_id, cname, sname, 
      branch_id)
    values (#{deptId,jdbcType=INTEGER}, #{cname,jdbcType=VARCHAR}, #{sname,jdbcType=VARCHAR}, 
      #{branchId,jdbcType=INTEGER})
  </insert>
    <insert id="insertSelective" parameterType="whu.iss.insurancesys.entity.DepartmentInfo">
        insert into department_info
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="deptId != null">
                dept_id,
            </if>
            <if test="cname != null">
                cname,
            </if>
            <if test="sname != null">
                sname,
            </if>
            <if test="branchId != null">
                branch_id,
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="deptId != null">
                #{deptId,jdbcType=INTEGER},
            </if>
            <if test="cname != null">
                #{cname,jdbcType=VARCHAR},
            </if>
            <if test="sname != null">
                #{sname,jdbcType=VARCHAR},
            </if>
            <if test="branchId != null">
                #{branchId,jdbcType=INTEGER},
            </if>
        </trim>
    </insert>
</mapper>